{
  "name": "react-starter-kit",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "hex-rgb": "^1.0.0",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel-core": "^5.6.15",
    "babelify": "^6.1.2",
    "browserify": "^10.2.6",
    "errorify": "^0.2.4",
    "exorcist": "^0.4.0",
    "http-server": "^0.8.0",
    "livereactload": "^0.6.0",
    "parallelshell": "^2.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.2.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify index.js --debug -g uglifyify | exorcist bundle.js.map > bundle.js",
    "build-dev": "browserify index.js --debug > bundle.js",
    "watch": "watchify index.js -g livereactload -d -v -o bundle.js -p errorify",
    "http-server": "http-server -p 8888",
    "reactload": "livereactload monitor -n bundle.js",
    "dev": "parallelshell \"npm run watch\" \"npm run reactload\" \"npm run http-server\""
  },
  "license": "MIT"
}
