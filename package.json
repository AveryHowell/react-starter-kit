{
  "name": "react-starter-kit",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "hex-rgb": "^1.0.0",
    "react": "0.14",
    "react-dom": "0.14"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "babel-core": "^5.6.15",
    "babel-plugin-react-transform": "^1.1.1",
    "babelify": "^6.1.2",
    "browserify": "^12.0.1",
    "dotenv": "^1.2.0",
    "errorify": "^0.3.1",
    "exorcist": "^0.4.0",
    "http-server": "^0.8.0",
    "livereactload": "^2.1.0",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.1.0",
    "postcss-custom-properties": "^5.0.0",
    "react-proxy": "^1.1.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.2.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "check_env": "node check_env.js",
    "check_env:windows": "set NODE_ENV=development&& node check_env.js",
    "css": "postcss --watch -c postcss.json",
    "js:build": "browserify index.js --debug -t babelify -g uglifyify | exorcist dist/bundle.js.map > dist/bundle.js",
    "js:build-dev": "browserify index.js --debug > dist/bundle.js",
    "js:watch": "watchify index.js -t babelify -p livereactload -d -v -o dist/bundle.js -p errorify",
    "http-server": "http-server -p 8888",
    "dev": "parallelshell \"npm run css\" \"npm run js:watch\" \"npm run http-server\""
  },
  "license": "MIT"
}
